            <h2 class="page-title">Crear espacio</h2>
            <div class="card shadow mb-4">
              {{!-- <div class="card-header">
                <strong class="card-title">Registrar un nuevo propietario</strong>
              </div> --}}
              <div class="card-body">
                <div class="row">
                  <div class="col-md-6">
                    <div class="form-group mb-3">
                      <label for="simpleinput">Tipo espacio</label>
                      <select name="" id="simpleinput" class="form-control">
                        <option value="null">ESCOGE TIPO DE ESPACIO</option>
                        <option value="APARTAMENTO">APARTAMENTO</option>
                        <option value="PARQUEADERO">PARQUEADERO</option>
                        <option value="SALON SOCIAL">SALON SOCIAL</option>
                        <option value="ZONA HUMEDA">ZONA HUMEDA</option>

                      </select>
                    </div>
                    

                    <div class="form-group mb-3">
                      <label for="example-password">Nombre espacio</label>
                      <input type="text" id="nombre_espacio" class="form-control" onchange="ER_nombre_espacio()" required>
                      <div id="FB_nombre_espacio"></div>
                    </div>





                  </div> <!-- /.col -->


                  <div class="col-md-6">


                    <div class="form-group mb-3">
                      <label for="example-password">Area espacio</label>
                      <input type="number" id="area" class="form-control" onchange="ER_area()" required>
                      <div id="FB_area"></div>
                    </div>

                    <div class="form-group mb-3">
                      <label for="example-password">Capacidad</label>
                      <input type="number" id="capacidad" class="form-control" onchange="ER_capacidad()" required>
                      <div id="FB_capacidad"></div>
                    </div>

                    <div class="form-group mb-3">
                      <label>Estado</label>
                      <select name="" id="simpleinput" class="form-control">
                        <option value="ACTIVO">ACTIVO</option>
                        <option value="INACTIVO">INACTIVO</option>
                      </select>
                    </div>


                  </div>
                  <div class="card-body">
                    <button id="boton_crear" type="button" class="btn mb-2 btn-outline-primary btn-block"><span
                        class="fe fe-arrow-right fe-16 mr-2"></span>Crear</button>
                    <button id="regresar" type="button" class="btn mb-2 btn-outline btn-block"><span
                        class="fe fe-arrow-left fe-16 mr-2"></span>Regresar</button>
                  </div>
                </div>

