  {{!-- Modal editar --}}


  <div class="modal fade" id="eventModal" tabindex="-1" role="dialog" aria-labelledby="eventModalLabel"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="varyModalLabel">Editar propietario</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">×</span>
          </button>
        </div>
        <div class="modal-body p-4">

          <div class="form-group mb-3">
            <input id="_id" type="hidden">
            <label for="simpleinput">Tipo de Documento</label>
            <select name="" id="tipo_documento_propietario" class="form-control">
              <option value="">Selecione un tipo de documento</option>
              <option value="CC">CEDULA DE CIUDADANIA</option>
              <option value="CE">CEDULA EXTRANJERIA</option>
            </select>
            <div id="FB_tipo_documento"></div>
          </div>

          <div class="form-group mb-3">
            <label for="documento">Numero documento</label>
            <input type="number" id="numero_documento_propietario" class="form-control" required>
            <div id="FB_documento"></div>
          </div>

          <div class="form-group mb-3">
            <label for="example-password">Nombre propietarios</label>
            <input type="text" id="nombre_propietario" class="form-control" required>
            <div id="FB_nombre"></div>
          </div>

          <div class="form-group mb-3">
            <label for="example-password">Apellido propietarios</label>
            <input type="text" id="apellido_propietario" class="form-control" required>
            <div id="FB_apellido"></div>
          </div>

          <div class="form-group mb-3">
            <label for="example-password">Fecha nacimiento</label>
            <input type="date" id="fecha_nacimiento" class="form-control" required>
            <div id="FB_fecha_nacimiento"></div>
          </div>

          <div class="form-group mb-3">
            <label>Genero</label>
            <select name="" id="genero_propietario" class="form-control">
              <option value="">Seleccione un genero</option>
              <option value="M">MASCULINO</option>
              <option value="F">FEMENINO</option>
              <option value="otro">OTRO</option>
            </select>
            <div id="FB_genero_propietario"></div>
          </div>

          <div class="form-group mb-3">
            <label>Telefono</label>
            <input type="text" id="telefono_propietario" class="form-control">
            <div id="FB_telefono"></div>
          </div>

          <div class="form-group mb-3">
            <label>Correo</label>
            <input type="email" id="correo" class="form-control" required>
            <div id="FB_correo"></div>
          </div>


          <div class="form-group mb-3">
            <label for="simpleinput">Propiedad</label>
            <select name="" id="propiedad" class="form-control">
              <option value="null">Selecciona propiedad</option>
              {{!-- Aqui va codigo para listar espacios --}}
            </select>
          </div>

          <div class="form-group mb-3">
            <label>Estado</label>
            <select name="" id="estado" class="form-control">
              <option value="ACTIVO">ACTIVO</option>
              <option value="INACTIVO">INACTIVO</option>
            </select>
          </div>

        </div>

        <div class="modal-footer d-flex justify-content-between">
          <button onclick="actualizar_propietario()" id="boton_actualizar" type="button"
            class="btn mb-2 btn-primary"><span class="fe mr-2"></span>Actualizar
            propietario</button>
